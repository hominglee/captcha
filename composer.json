{
    "name": "realhome/captcha",
    "description": "Laravel 5 Captcha Package Extend",
    "keywords": ["laravel", "captcha", "redis"],
    "homepage": "https://github.com/realhome/captcha",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Realhome\\Captcha\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "Realhome\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Realhome\\Captcha\\CaptchaServiceProvider"
            ],
            "aliases": {
                "Captcha": "Realhome\\Captcha\\Facades\\Captcha"
            }
        }
    },
    "minimum-stability": "dev"
}
